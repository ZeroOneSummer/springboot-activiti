<?xml version="1.0" encoding="UTF-8" ?>        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.act.activiti.dao.ActRequestDao">    <resultMap id="base" type="ActRequest">        <result property="requestId" column="request_id"/>        <result property="requestType" column="request_Type"/>        <result property="requestUrl" column="request_url"/>        <result property="requestaram" column="request_param"/>        <result property="addTime" column="add_time"/>        <result property="upTime" column="up_time"/>        <result property="addName" column="add_name"/>        <result property="remark" column="remark"/>        <result property="reqName" column="req_name"/>        <result property="keyName" column="key_name"/>        <result property="getOrder" column="get_order"/>        <result property="auditFlagNo" column="audit_flagno"/>    </resultMap>    <sql id="baseSql">        request_id,request_type,request_url,request_param,add_time,up_time,        add_name,remark,req_name,key_name,get_order,audit_flagno    </sql>    <select id="findByParam" parameterType="ActRequest" resultMap="base">        SELECT        <include refid="baseSql"/>        FROM        ACT_REQUEST        WHERE        <if test="keyName != null and getOrder != null">            key_name = #{keyName} AND get_order = #{getOrder}        </if>    </select>    <select id="findListByParam" parameterType="ActRequest" resultMap="base">        SELECT        <include refid="baseSql"/>        FROM        ACT_REQUEST r        <if test="getOrder != null">            WHERE get_order = #{getOrder}        </if>    </select></mapper>